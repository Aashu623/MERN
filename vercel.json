{
    "version": 2,
    "builds": [
        {
            "src": "brahmi-frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}